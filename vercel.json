{
  "version": 2,
  "name": "natu-helados",
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "scripts.js", "use": "@vercel/static" },
    { "src": "productos.js", "use": "@vercel/static" },
    { "src": "styles.css", "use": "@vercel/static" },
    { "src": "outputs/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}